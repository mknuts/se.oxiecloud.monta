{
  "name": {
    "en": "Monta EV Charger",
    "se": "Monta laddbox"
  },
  "class": "evcharger",
  "capabilities": [
  
    "evcharger_charging",
    "charging_state",
    "charger_state",
    "connected",
    "measure_power",
    "meter_power",
    "measure_monetary",
    "meter_lastkwh"
    
  ],
  "capabilitiesOptions": {
    "evcharger_charging_state": {
      "type": "enum",
      "title": { "en": "Charger State", "sv": "Laddboxens status" },
      "values": [
        { "id": "available", "title": { "en": "Available", "sv": "Ledig" } },
        { "id": "busy", "title": { "en": "Busy", "sv": "Upptagen" } },
        { "id": "busy-charging", "title": { "en": "Charging", "sv": "Laddar" } },
        { "id": "busy-scheduled", "title": { "en": "Busy Scheduled", "sv": "Schemalagd" } },
        { "id": "disconnected", "title": { "en": "Disconnected", "sv": "Frånkopplad" } },
        { "id": "error", "title": { "en": "Error", "sv": "Fel" } }
      ]
    },
    "ev_charging_state": {
      "type": "enum",
      "title": { "en": "Charging Mode", "sv": "Laddningsläge" },
      "values": [
        { "id": "reserved", "title": { "en": "Reserved", "sv": "Reserverad" } },
        { "id": "starting", "title": { "en": "Starting", "sv": "Startar" } },
        { "id": "charging", "title": { "en": "Charging", "sv": "Laddar" } },
        { "id": "stopping", "title": { "en": "Stopping", "sv": "Stoppar" } },
        { "id": "paused", "title": { "en": "Paused", "sv": "Pausad" } },
        { "id": "scheduled", "title": { "en": "Scheduled", "sv": "Schemalaggd" } },
        { "id": "stopped", "title": { "en": "Stopped", "sv": "Stoppad" } },
        { "id": "completed", "title": { "en": "Completed", "sv": "Genomförd" } }
      ]
    },
    "measure_monetary": {
      "title": { "en": "Charge cost", "sv": "Laddkostnad" }
    }

  },
  "energy": {
    "evCharger": true,
    "meterPowerImportedCapability": "meter_power"
  },

  "platforms": [
    "local"
  ],
  "connectivity": [
    "cloud"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/driver_small.png",
    "large": "{{driverAssetsPath}}/images/driver_large.png"
  },
  "pair": [
    
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ]
}